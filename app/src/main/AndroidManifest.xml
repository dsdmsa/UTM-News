        android:supportsRtl="true"
        android:name="dsdmsa.utmnews.This"
        android:theme="@style/AppTheme">
        <activity android:name="dsdmsa.utmnews.activityes.SplashScreenActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="dsdmsa.utmnews.activityes.main.MainActivity"/>
        <activity android:name="dsdmsa.utmnews.activityes.categories.CategoriesActivity"/>
        <activity android:name="dsdmsa.utmnews.activityes.search.SearchActivity"/>
        <activity android:name="dsdmsa.utmnews.activityes.about.AboutActivity"/>
        <activity android:name="dsdmsa.utmnews.activityes.PrefActivity"/>
        <activity android:name="dsdmsa.utmnews.activityes.news.NewsListActivity"/>
